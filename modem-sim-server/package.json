{
  "name": "creditpro-sim-manager",
  "version": "1.0.0",
  "description": "CreditPro SIM Manager - Backend Node.js pour gestion des modems GSM",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "clean": "rm -rf dist node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install"
  },
  "keywords": ["gsm", "modem", "ussd", "sim", "creditpro"],
  "author": "CreditPro Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "serialport": "^12.0.0",
    "@serialport/parser-readline": "^12.0.0",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.8",
    "@types/uuid": "^9.0.5",
    "typescript": "^5.2.2",
    "tsx": "^4.7.0",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5"
  }
}